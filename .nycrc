{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": ["src/**/*.ts"],
  "exclude": ["test/**/*.ts", "src/**/*.d.ts"],
  "reporter": ["html", "text-summary"],
  "extension": [".ts"]
}
